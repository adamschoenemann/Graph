package dk.as.graph2;

import java.util.List;

public interface Graph {
	Vertex getVertex(int i);
	Edge getEdge(int i);
	int numVertices();
	int numEdges();
	Edge connect(Vertex a, Vertex b);
	void addVertex(Vertex v);

	public interface Vertex {
		List<Edge> getEdges();
		int numEdges();
		Edge connect(Vertex other);
	}

	public interface Edge {
		Vertex getHead();
		Vertex getTail();
		Vertex getNot(Vertex not);
	}

}
